{
  "name": "cdk-ec2-key-pair",
  "description": "CDK Construct for managing EC2 key pairs",
  "version": "5.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-ec2-key-pair",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-ec2-key-pair.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-ec2-key-pair/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "aws-cdk-construct",
    "ec2",
    "ec2-key-pair",
    "cloudformation",
    "cloudformation-custom-resource"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda",
      "test"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-ec2-key-pair",
        "module": "cdk_ec2_key_pair"
      },
      "dotnet": {
        "namespace": "CDK.EC2.KeyPair",
        "packageId": "CDK.EC2.KeyPair"
      }
    }
  },
  "devDependencies": {
    "@aws-sdk/client-ec2": "3.938.0",
    "@aws-sdk/client-secrets-manager": "3.936.0",
    "@swc/core": "1.15.3",
    "@swc/helpers": "0.5.17",
    "@types/node": "24.9.1",
    "@types/node-forge": "1.3.14",
    "@types/sshpk": "1.17.4",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "aws-cdk-lib": "2.223.0",
    "aws-cloudformation-custom-resource": "5.0.0",
    "constructs": "10.4.2",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jsii": "5.9.14",
    "jsii-pacmak": "1.118.0",
    "node-forge": "1.3.1",
    "prettier": "3.6.2",
    "publib": "0.2.1025",
    "regenerator-runtime": "0.14.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.219.0",
    "constructs": "^10.0.0"
  }
}
